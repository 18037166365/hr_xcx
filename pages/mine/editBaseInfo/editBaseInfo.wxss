.form-title{
  font-size:26rpx;
  color: #999;
  padding:36rpx 0 24rpx 30rpx;
}
.weui-label::before{
  content:"*";
  color:#F96868;
  font-size:30rpx;
  margin-right: 4rpx;
}
.weui-icon-clear{
  right:20rpx;
  top:50%;
  transform: translateY(-50%);
  z-index:999;
  
}
.email-input .weui-icon-clear,.phone-input .weui-icon-clear{
  right:40px;
}
.weui-cell__ft {
  width:21px;
  height: 21px;
  overflow: visible;
}
.weui-cell__ft icon{
  vertical-align:top;

} 
.placeholder{
  color: #777;
}
.picker-inner{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width:100%;
  height:2.58823529em;
  min-height:2.58823529em;
  line-height:2.58823529em;
}
.arrow-right{
  margin-right:10rpx;
}
.save-btn{
  margin-top:60rpx; 
}